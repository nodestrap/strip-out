{
  "name": "@nodestrap/strip-out",
  "version": "3.0.0",
  "description": "Removes any browser's default styling on a particular element.",
  "dependencies": {},
  "devDependencies": {
    "tilde-sass": "^1.0.1"
  },
  "scripts": {
    "test": "tilde-sass --file test/*.scss --outFile test/ --mergeSelectors",
    "test-watch": "tilde-sass --file test/*.scss --outFile test/ --mergeSelectors --watch",
    "compile": "tilde-sass --file src/*.scss --outFile dist/ --mergeSelectors --outputStyle compressed",
    "compile-watch": "tilde-sass --file src/*.scss --outFile dist/ --mergeSelectors --outputStyle compressed --watch"
  },
  "cssnano": {
    "preset": [
      "default",
      {
        "calc": false
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodestrap/strip-out.git"
  },
  "keywords": [
    "css",
    "browser",
    "user",
    "agent",
    "default",
    "style",
    "reset",
    "reboot",
    "remove"
  ],
  "author": "nodestrap, heymarco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nodestrap/strip-out/issues"
  },
  "homepage": "https://github.com/nodestrap/strip-out#readme"
}
